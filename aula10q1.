maio=0
nome=[]
preco=[]
for i in range(11):
    nome = input(f"Digite nome do  medicamento{i+1}º: ")
    preco= float(input(f"Digite preço do  medicamento{i+1}º: "))
    if preco > maio:
        maio = preco
    else:
      print(f"nome e o preço do medicamento maiois barato e {nome} e o preço e {preco:.2f}")
